SELECT human_humanid FROM contactemail INNER JOIN human ON (human_humanid = humanid and emailid = 104);  
SELECT human_humanid FROM contactphone INNER JOIN human ON (human_humanid = humanid and phoneid = 116);
SELECT human_humanid FROM humanmuni     INNER JOIN human ON  (human_humanid = humanid and linkid = 1044);
SELECT human_humanid FROM humancecase  INNER JOIN human ON (human_humanid = humanid  and cecase_caseid = 1044);
SELECT human_humanid FROM humanparcel  INNER JOIN human ON  (human_humanid = humanid and linkid = 61987);
SELECT human_humanid FROM humanoccperiod INNER JOIN human ON  (human_humanid = humanid and linkid = 1044);
SELECT human_humanid FROM humanparcelunit INNER JOIN human ON  (human_humanid = humanid and linkid = 1044); 


