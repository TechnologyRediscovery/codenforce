UPDATE citationdocketno SET notes = 'this is a test 514v1' WHERE docketid = 102; 
UPDATE citationdocketnohuman SET notes = 'this is a test 514v2' WHERE linkid = 1026;  
UPDATE citationcitationstatus SET notes = 'test of new trigr' WHERE citation_citationid = 1044;

SELECT caseid FROM cecase INNER JOIN codeviolation ON  (caseid = cecase_caseid and caseid = 1013);
SELECT cecase_caseid FROM codeviolation INNER JOIN citationviolation ON (violationid = codeviolation_violationid and cecase_caseid = 1027;
SELECT cecase_caseid FROM codeviolation INNER JOIN citation ON (citation_citationid = citationid and linkid = 61987);			     
SELECT cecase_caseid FROM codeviolation INNER JOIN citationdocketno ON (citation_citationid = citationid and docketid=102);
SELECT cecase_caseid lastupdatedts FROM codeviolation INNER JOIN citationdocketno ON (citation_citationid = citation_citationid and docketid=102);
SELECT cecase_caseid lastupdatedts FROM codeviolation INNER JOIN citationdocketno ON (citation_citationid = citation_citationid and docketid=102);
SELECT cecase_caseid  FROM codeviolation INNER JOIN citationdocketno ON (citation_citationid = citation_citationid and docketid=102);
