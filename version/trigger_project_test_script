UPDATE parcelinfo SET notes = 'this is a test 513v1' WHERE parcelinfoid = 29171; 
UPDATE parcelunit SET notes = 'this is a test 513v1' WHERE unitid = 1044; 
UPDATE humanparcel  SET notes = 'this is a test 513v1' WHERE linkid = 61997;
UPDATE parcelmailingaddress SET notes = 'this is a test 513v1' WHERE mailingparcel_parcelid = 130055;  
UPDATE parcelphotodoc SET photdoc_photodocid = 98 WHERE parcel_parcelkey = 1044; 
UPDATE parcel SET notes = 'this is a test 513v1' WHERE parcelkey = 2387; 

SELECT parcel_parcelkey FROM parcelinfo INNER JOIN parcel ON  (parcel_parcelkey = parcelkey and parcelinfoid = 29171); 
SELECT parcel_parcelkey FROM parcelmailingaddress INNER JOIN parcelunit ON (mailingparcel_mailingid = address_parcelmailingid and address_parcelmailingid = 130055);
SELECT parcel_parcelkey FROM humanparcel  INNER JOIN parcel ON (parcel_parcelkey = parcelkey and linkid = 61987);
SELECT parcel_parcelkey FROM parcelphotodoc INNER JOIN parcel ON (parcel_parcelkey = parcelkey and parcel_parcelkey = 1044);
SELECT parcelkey FROM parcel WHERE parcelkey = 2387; 

UPDATE occpermit SET notes = 'this is a test 514v1' WHERE unitid = 1044; 
UPDATE occperiod SET notes = 'this is a test 514v1' WHERE periodid = 1006; 
UPDATE occperiodpermitapplication SET notes = 'this is a test 514v1' WHERE applicationid = 1011; 
UPDATE occperiodphotodoc SET photodoc_photodocid = 102 WHERE unitid = 1044;
UPDATE occinspection SET notes = 'this is a test 514v1' WHERE inspectionid = 15;
--UPDATE occperiod to get event trigger to occur


SELECT occperiod_periodid FROM occpermit WHERE (permitid = 1044);
SELECT occperiod periodid FROM occperiod WHERE (periodid = 1006);
SELECT occperiod_periodid FROM occperiodpermitapplication WHERE (occperiod_periodid = 29171);
SELECT occperiod_periodid FROM occperiodphotodoc  WHERE photodoc_photodocid = 1044;
SELECT occperiod_periodid FROM occinspection WHERE inspectionid = 15;


